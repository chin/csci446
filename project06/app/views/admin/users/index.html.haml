%h1 Listing users
%p#notice= notice
%table
  %tr
    %th Username
    %th Email
    %th
    %th
    %th
  - @users.each do |user|
    %tr
      %td= user.username
      %td= user.email
      %td= link_to 'Show', user
      %td= link_to 'Edit', edit_user_path(user)
      %td= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete
%br/
